             x:Class="Sakamichi46Mobile.Nogizaka46.NogiDetailPage"
             Title="Detail">

  <TabbedPage.Children>
    <ContentPage Title="Blog">
      <WebView x:Name="nogiWebBlog" />
    </ContentPage>
    <ContentPage Title="Goods">
      <WebView x:Name="nogiWebGoods" />
    </ContentPage>
    <ContentPage Title="YouTube">
